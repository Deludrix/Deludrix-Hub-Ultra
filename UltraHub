WorkingTab:AddButton({
    Name = "Give Key",
    Callback = function()
        OrionLib:MakeNotification({
            Name = "This is in WIP",
            Content = "Script not released yet",
            Image = "rbxassetid://4483345998",
            Time = 5
        })
    end
})

WorkingTabTab:AddDropdown({
	Name = "Give Item (WIP)",
	Default = "1",
	Options = {"None", "Lockpicks", "Vitamins", "Crucifix", "Skeleton Key", "More Items Later"},
	Callback = function(Value)
        Name = "This is in WIP",
        Content = "Script not released yet",
        Image = "rbxassetid://4483345998",
        Time = 5
	end    
})

